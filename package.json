{
  "name": "sarl-jeremie-arrive",
  "version": "1.0.0",
  "description": "Site vitrine moderne pour SARL Jérémie Arrivé - Architecture Frontend/Backend",
  "private": true,
  "scripts": {
    "install": "npm install --prefix backend && npm install --prefix frontend",
    "start": "npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:frontend"
  },
  "keywords": [
    "plomberie",
    "chauffage",
    "climatisation",
    "renovation",
    "la-tremblade",
    "charente-maritime"
  ],
  "author": "SARL Jérémie Arrivé",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}
